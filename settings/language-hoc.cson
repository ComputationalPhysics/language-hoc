'.source.hoc':
  'editor':
    'autoIndentOnPaste': false
    'softTabs': true
    'tabLength': 4
    'foldEndPattern': '^\\s*\\}\\s*$'
    'commentStart': '// '
    'increaseIndentPattern': '^\\s*(class|def|elif|else|except|finally|for|if|try|with|while)\\b.*\\{\\s*$'
    'decreaseIndentPattern': '^\\s*\\}\\b.*:'
